{
  "name": "functions",
  "version": "0.63.0",
  "scripts": {
    "lint": "npx eslint src/**/*.ts --fix",
    "build": "rm -rf lib && echo '{\"version\"': '\"'$(npm --no-git-tag-version version minor)'\"}' > src/version.json && tsc",
    "buildw": "rm -rf lib && (for /f \"delims=\" %A in ('npm --no-git-tag-version version minor') do @echo {\"version\":\"%A\"}) > src/version.json && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && npx firebase emulators:start --only functions",
    "serve1": "npx firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npx firebase deploy --only functions",
    "logs": "npx firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-plugin-n": "^17.17.0",
    "firebase-functions-test": "^3.4.1",
    "typescript": "^5.8.3"
  },
  "private": true
}
